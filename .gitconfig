[init]
	defaultBranch = main
[user]
	name = Leonardo Ralph
	email = theleoralph@gmail.com
[core]
	editor = nvim
[alias]
  co = checkout
  lg = log --all --graph --decorate --oneline --abbrev-commit
  cm = commit
  ac = !git add -A && git commit
  st = status -sb
  tags = tag -l
  branches = branch -a
  remotes = remote -v
